<% cache student do %>
  <tr>
    <td><%= student.first_name %></td>
    <td><%= student.last_name %></td>
    <td><%= student.email %></td>
    <td>
      <%= link_to 'Show', [@dojo, student] %> | <%= link_to 'Edit', edit_dojo_student_path(@dojo.id, student.id) %>
      <%= link_to 'Destroy', [@dojo, student],
        data: {:confirm => 'Are you sure?'}, :method => :delete %>
    </td>
  </tr>
<% end %>
