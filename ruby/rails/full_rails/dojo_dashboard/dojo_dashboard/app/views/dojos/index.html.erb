<h1>Dojos#index</h1>
<table>
  <thead>
    <tr>
      <th>Branch</th>
      <th>Street</th>
      <th>City</th>
      <th>State</th>
      <th>Actions</th>
    </tr>
  </thead>
  <tbody>
    <% @dojos.each do |dojo| %>
      <tr>
        <td><%= dojo.branch %></td>
        <td><%= dojo.street %></td>
        <td><%= dojo.city %></td>
        <td><%= dojo.state %></td>
        <td>
          <%= link_to 'Show', dojo_path(dojo)%> | <%= link_to 'Edit', edit_dojo_path(dojo) %>
          <%= link_to 'Destroy', dojo_path(dojo),
            data: {:confirm => 'Are you sure?'}, :method => :delete %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>
<%= link_to 'Add Dojo', new_dojo_path %>
